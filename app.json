{
  "expo": {
    "name": "home",
    "slug": "home",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/icon.png",
    "scheme": "home",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/shared/assets/images/android-icon-foreground.png",
        "backgroundImage": "./src/shared/assets/images/android-icon-background.png",
        "monochromeImage": "./src/shared/assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.vik163.home",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./src/shared/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./src/shared/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./src/shared/assets/fonts/Alegreya-Black.ttf"],
          "android": {
            "fonts": [
              {
                "fontFamily": "Roboto",
                "fontDefinitions": [
                  {
                    "path": "./src/shared/assets/fonts/Roboto-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./src/shared/assets/fonts/Roboto-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./src/shared/assets/fonts/Roboto-Black.ttf",
                    "weight": 900
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              ".src/shared/assets/fonts/Roboto-Medium.ttf",
              ".src/shared/assets/fonts/Roboto-Bold.ttf"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3529226c-7bfb-4a42-a532-35313d479eb9"
      }
    }
  }
}
