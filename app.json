{
  "expo": {
    "name": "home",
    "slug": "home",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/images/icon.png",
    "scheme": "home",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/shared/assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.vik163.home"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/shared/assets/images/android-icon-background.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./src/shared/assets/fonts/SofiaSans-Black.ttf"],
          "android": {
            "fonts": []
          },
          "ios": {
            "fonts": []
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3529226c-7bfb-4a42-a532-35313d479eb9"
      }
    }
  }
}
